{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "lib": ["es2018", "dom"],
    "outDir": "build",
    "allowJs": false,
    "esModuleInterop": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
